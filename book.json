{
    "gitbook": "2.2.0",
    "plugins": ["layout", "anchors", "ga", "video", "addcssjs"],
    "structure": {
        "readme": "welcome.md"
    },
    "pluginsConfig": {
        "layout": {
            "headerPath" : "layouts/header.html",
            "footerPath" : "layouts/footer.html"
